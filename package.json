{
  "name": "micro-brreg",
  "description": "microservice for brreg",
  "version": "3.3.1",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Geir Gåsodden",
    "email": "geir.gasodden@pythonia.no",
    "url": "https://github.com/zrrrzzt"
  },
  "main": "index.js",
  "engines": {
    "node": ">=10.12.0"
  },
  "scripts": {
    "test": "standard && npm audit && ava",
    "test-offline": "standard && ava",
    "coverage": "nyc ava",
    "coveralls": "nyc ava && nyc report --reporter=lcov && cat coverage/lcov.info | coveralls",
    "standard-fix": "standard --fix",
    "deploy": "npm test && now && now alias",
    "dev": "micro-dev",
    "start": "micro",
    "refresh": "rm -rf node_modules && rm package-lock.json && npm install"
  },
  "keywords": [
    "microservice",
    "brreg",
    "brønnøysund",
    "norwegian entity registry"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zrrrzzt/micro-brreg.git"
  },
  "bugs": {
    "url": "https://github.com/zrrrzzt/micro-brreg/issues"
  },
  "homepage": "https://github.com/zrrrzzt/micro-brreg#readme",
  "devDependencies": {
    "ava": "1.0.1",
    "coveralls": "3.0.2",
    "husky": "1.3.1",
    "micro-dev": "3.0.0",
    "nyc": "13.1.0",
    "standard": "12.0.1",
    "test-listen": "1.1.0"
  },
  "dependencies": {
    "axios": "0.18.0",
    "markdown-it": "8.4.2",
    "micro": "9.3.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  }
}
