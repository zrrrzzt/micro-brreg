{
  "name": "micro-brreg",
  "description": "microservice for brreg",
  "version": "3.2.1",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Geir Gåsodden",
    "email": "geir.gasodden@pythonia.no",
    "url": "https://github.com/zrrrzzt"
  },
  "main": "index.js",
  "engines": {
    "node": ">=8.6.0"
  },
  "scripts": {
    "test": "standard && nsp check && ava",
    "test-offline": "standard && ava",
    "coverage": "nyc ava",
    "coveralls": "nyc ava && nyc report --reporter=lcov && cat coverage/lcov.info | coveralls",
    "setup": "npm install",
    "standard-fix": "standard --fix",
    "now-deploy": "npm test && now --npm && now alias",
    "dev": "micro-dev",
    "start": "micro"
  },
  "keywords": [
    "microservice",
    "brreg",
    "brønnøysund",
    "norwegian entity registry"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/zrrrzzt/micro-brreg.git"
  },
  "bugs": {
    "url": "https://github.com/zrrrzzt/micro-brreg/issues"
  },
  "homepage": "https://github.com/zrrrzzt/micro-brreg",
  "now": {
    "alias": [
      "organisasjonsnummer.mikrotjeneste.no"
    ],
    "env": {
      "NODE_ENV": "production"
    },
    "type": "npm"
  },
  "devDependencies": {
    "ava": "0.22.0",
    "coveralls": "3.0.0",
    "micro-dev": "1.2.3",
    "nsp": "2.8.1",
    "nyc": "11.2.1",
    "standard": "10.0.3",
    "test-listen": "1.0.2"
  },
  "dependencies": {
    "axios": "0.16.2",
    "markdown-it": "8.4.0",
    "micro": "9.0.0"
  }
}
