{
  "name": "micro-brreg",
  "description": "microservice for brreg",
  "version": "3.2.2",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Geir Gåsodden",
    "email": "geir.gasodden@pythonia.no",
    "url": "https://github.com/zrrrzzt"
  },
  "main": "index.js",
  "engines": {
    "node": ">=8.9.4"
  },
  "scripts": {
    "test": "standard && nsp check && ava",
    "test-offline": "standard && ava",
    "coverage": "nyc ava",
    "coveralls": "nyc ava && nyc report --reporter=lcov && cat coverage/lcov.info | coveralls",
    "setup": "npm install",
    "standard-fix": "standard --fix",
    "now-deploy": "npm test && now && now alias",
    "dev": "micro-dev",
    "start": "micro",
    "refresh": "rm -rf node_modules && rm package-lock.json && npm install"
  },
  "keywords": [
    "microservice",
    "brreg",
    "brønnøysund",
    "norwegian entity registry"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zrrrzzt/micro-brreg.git"
  },
  "bugs": {
    "url": "https://github.com/zrrrzzt/micro-brreg/issues"
  },
  "homepage": "https://github.com/zrrrzzt/micro-brreg#readme",
  "now": {
    "alias": [
      "organisasjonsnummer.mikrotjeneste.no"
    ],
    "env": {
      "NODE_ENV": "production"
    },
    "type": "npm",
    "engines": {
      "node": "8.9.4"
    }
  },
  "devDependencies": {
    "ava": "0.25.0",
    "coveralls": "3.0.0",
    "micro-dev": "2.2.0",
    "nsp": "3.1.0",
    "nyc": "11.4.1",
    "standard": "10.0.3",
    "test-listen": "1.1.0"
  },
  "dependencies": {
    "axios": "0.17.1",
    "markdown-it": "8.4.0",
    "micro": "9.1.0"
  }
}
