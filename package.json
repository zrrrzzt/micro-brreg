{
  "name": "micro-brreg",
  "description": "microservice for brreg",
  "version": "3.1.0",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Geir Gåsodden",
    "email": "geir.gasodden@pythonia.no",
    "url": "https://github.com/zrrrzzt"
  },
  "main": "index.js",
  "engines": {
    "node": ">=7.7.1"
  },
  "scripts": {
    "test": "standard && nsp check && ava",
    "test-offline": "standard && ava",
    "coverage": "nyc ava",
    "coveralls": "nyc ava && nyc report --reporter=lcov && cat coverage/lcov.info | coveralls",
    "setup": "npm install",
    "standard-fix": "standard --fix",
    "now-deploy": "npm test && now --npm && now alias",
    "start": "micro"
  },
  "keywords": [
    "microservice",
    "brreg",
    "brønnøysund",
    "norwegian entity registry"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/zrrrzzt/micro-brreg.git"
  },
  "bugs": {
    "url": "https://github.com/zrrrzzt/micro-brreg/issues"
  },
  "now": {
    "alias": [
      "organisasjonsnummer.mikrotjeneste.win"
    ],
    "env": {
      "NODE_ENV": "production"
    },
    "type": "npm"
  },
  "devDependencies": {
    "ava": "0.18.2",
    "coveralls": "2.12.0",
    "nsp": "2.6.3",
    "nyc": "10.1.2",
    "standard": "9.0.0",
    "test-listen": "1.0.1"
  },
  "dependencies": {
    "axios": "0.15.3",
    "marked": "0.3.6",
    "micro": "7.2.1"
  }
}
